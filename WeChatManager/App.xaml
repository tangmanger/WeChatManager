<Application x:Class="WeChatManager.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:WeChatManager"
             xmlns:vm="clr-namespace:WeChatManager.ViewModels"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary>
                    <vm:ViewModelLocator x:Key="Locator"></vm:ViewModelLocator>
                </ResourceDictionary>
                <ResourceDictionary Source="/Files/Langs/Zh_CN.xaml"></ResourceDictionary>
                <ResourceDictionary Source="/Themes/DefaultColor.xaml"></ResourceDictionary>
                <ResourceDictionary Source="/Style/TextBlock.xaml"></ResourceDictionary>
                <ResourceDictionary Source="/Style/Button.xaml"></ResourceDictionary>
                <ResourceDictionary Source="/Style/Path.xaml"></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
